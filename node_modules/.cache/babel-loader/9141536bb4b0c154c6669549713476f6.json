{"ast":null,"code":"import axios from \"axios\";\nimport WebHeader from \"./WebHeader\";\nimport WebFooter from \"./WebFooter\"; // import api from \"@/api.js\";\n\nexport default {\n  name: \"TvMovies\",\n  components: {\n    WebHeader,\n    WebFooter\n  },\n\n  data() {\n    return {\n      movies: []\n    };\n  },\n\n  methods: {\n    getData() {\n      axios // .get(`https://imdb-api.com/API/AdvancedSearch/${api.apikey3}/?title_type=tv_movie`)\n      .get(\"http://localhost:8081/movie/tv_movies\").then(data => {\n        this.movies = data.data;\n      });\n    }\n\n  },\n\n  mounted() {\n    this.getData();\n  }\n\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,SAAP,MAAsB,aAAtB;AACA,OAAOC,SAAP,MAAsB,aAAtB,C,CACA;;AACA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AAAEH,aAAF;AAAaC;AAAb,GAFC;;AAGbG,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE;AADH,KAAP;AAGD,GAPY;;AAQbC,SAAO,EAAE;AACPC,WAAO,GAAG;AACRR,WAAI,CACF;AADE,OAEDS,GAFH,CAEO,uCAFP,EAGGC,IAHH,CAGSL,IAAD,IAAU;AACd,aAAKC,MAAL,GAAcD,IAAI,CAACA,IAAnB;AACD,OALH;AAMD;;AARM,GARI;;AAkBbM,SAAO,GAAG;AACR,SAAKH,OAAL;AACD;;AApBY,CAAf","names":["axios","WebHeader","WebFooter","name","components","data","movies","methods","getData","get","then","mounted"],"sourceRoot":"","sources":["D:\\Java\\movie-website-project-1\\src\\components\\TvMovies.vue"],"sourcesContent":["<template>\r\n  <WebHeader />\r\n  <h2>PHIM TRUYỀN HÌNH HOT</h2>\r\n  <div class=\"popular-film\">\r\n    <div class=\"movie-item\" v-for=\"movie in movies\" :key=\"movie.idAPI\">\r\n      <router-link :to=\"'/movie/' + movie.idAPI\" class=\"movie-link\">\r\n        <div class=\"poster\">\r\n          <img :src=\"movie.image\" alt=\"\" />\r\n          <p class=\"rank\"><strong>TVMovies</strong></p>\r\n        </div>\r\n        <div class=\"detail\">\r\n          <p>\r\n            <strong>{{ movie.title }}</strong>\r\n          </p>\r\n          <p><strong>Năm Phát Hành: </strong>{{ movie.description }}</p>\r\n        </div>\r\n      </router-link>\r\n    </div>\r\n  </div>\r\n  <WebFooter />\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport WebHeader from \"./WebHeader\";\r\nimport WebFooter from \"./WebFooter\";\r\n// import api from \"@/api.js\";\r\nexport default {\r\n  name: \"TvMovies\",\r\n  components: { WebHeader, WebFooter },\r\n  data() {\r\n    return {\r\n      movies: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      axios\r\n        // .get(`https://imdb-api.com/API/AdvancedSearch/${api.apikey3}/?title_type=tv_movie`)\r\n        .get(\"http://localhost:8081/movie/tv_movies\")\r\n        .then((data) => {\r\n          this.movies = data.data;\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.popular-film {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  margin-top: 50px;\r\n}\r\n.movie-item {\r\n  margin: 4px;\r\n}\r\n.poster img {\r\n  width: 250px;\r\n  height: 380px;\r\n  object-fit: cover;\r\n  border-radius: 5px;\r\n}\r\n.detail {\r\n  background-color: rgb(21, 133, 99);\r\n  color: #fff;\r\n  width: 250px;\r\n  height: 100px;\r\n  display: none;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 0;\r\n  transition: 1s;\r\n  padding: 5px;\r\n}\r\n.movie-link {\r\n  position: relative;\r\n}\r\n.movie-link:hover .detail {\r\n  display: block;\r\n}\r\n.rank {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 10;\r\n}\r\n.poster > p {\r\n  background: linear-gradient(\r\n    to bottom right,\r\n    rgb(8, 218, 183),\r\n    rgb(53, 197, 24)\r\n  );\r\n  color: rgb(170, 32, 32);\r\n  width: 90px;\r\n  height: 30px;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n}\r\nh2 {\r\n  margin-top: 50px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}