{"ast":null,"code":"import { onMounted, ref } from \"vue\";\nimport { getAuth, onAuthStateChanged, signOut } from \"firebase/auth\";\nimport { useRouter } from \"vue-router\";\nexport default {\n  name: \"WebHeader\",\n\n  setup() {\n    const isLoggedIn = ref(false);\n    const username = ref(\"\");\n    const userID = ref(\"\");\n    const userImg = ref(\"\");\n    const router = useRouter();\n    let auth;\n    onMounted(() => {\n      auth = getAuth();\n      onAuthStateChanged(auth, user => {\n        if (user) {\n          isLoggedIn.value = true;\n          username.value = user.displayName;\n          userID.value = user.uid;\n          userImg.value = user.photoURL; // console.log(user);\n        } else {\n          isLoggedIn.value = false;\n        }\n      });\n    });\n\n    const handleSignOut = () => {\n      signOut(auth).then(() => {\n        router.push(\"/login\");\n      });\n    };\n\n    return {\n      handleSignOut,\n      isLoggedIn,\n      username,\n      userID,\n      userImg\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAyKA,SAASA,SAAT,EAAoBC,GAApB,QAA+B,KAA/B;AACA,SAASC,OAAT,EAAkBC,kBAAlB,EAAsCC,OAAtC,QAAqD,eAArD;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,eAAe;AACbC,MAAI,EAAE,WADO;;AAEbC,OAAK,GAAG;AACN,UAAMC,UAAS,GAAIP,GAAG,CAAC,KAAD,CAAtB;AACA,UAAMQ,QAAO,GAAIR,GAAG,CAAC,EAAD,CAApB;AACA,UAAMS,MAAK,GAAIT,GAAG,CAAC,EAAD,CAAlB;AACA,UAAMU,OAAM,GAAIV,GAAG,CAAC,EAAD,CAAnB;AACA,UAAMW,MAAK,GAAIP,SAAS,EAAxB;AACA,QAAIQ,IAAJ;AACAb,aAAS,CAAC,MAAM;AACda,UAAG,GAAIX,OAAO,EAAd;AACAC,wBAAkB,CAACU,IAAD,EAAQC,IAAD,IAAU;AACjC,YAAIA,IAAJ,EAAU;AACRN,oBAAU,CAACO,KAAX,GAAmB,IAAnB;AACAN,kBAAQ,CAACM,KAAT,GAAiBD,IAAI,CAACE,WAAtB;AACAN,gBAAM,CAACK,KAAP,GAAeD,IAAI,CAACG,GAApB;AACAN,iBAAO,CAACI,KAAR,GAAgBD,IAAI,CAACI,QAArB,CAJQ,CAKR;AACF,SANA,MAMO;AACLV,oBAAU,CAACO,KAAX,GAAmB,KAAnB;AACF;AACD,OAViB,CAAlB;AAWD,KAbQ,CAAT;;AAcA,UAAMI,aAAY,GAAI,MAAM;AAC1Bf,aAAO,CAACS,IAAD,CAAP,CAAcO,IAAd,CAAmB,MAAM;AACvBR,cAAM,CAACS,IAAP,CAAY,QAAZ;AACD,OAFD;AAGD,KAJD;;AAKA,WAAO;AACLF,mBADK;